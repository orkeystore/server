{
  "name": "orkeystore",
  "version": "0.1.0",
  "description": "Out-of-the-box solution for crypto-keys management, storing and rotation.",
  "author": "Alexandr Verkhoturov",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "env DEV=1 SWAGGER=1 ENV_FILE=.develop.env nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:prodtest": "env ADMIN_USERNAME=admin ADMIN_PASSWORD=test KEYS_HOST=http://localhost:3000  DATABASE_PATH=/tmp/test.db node dist/main",
    "lint": "tsc && eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "env PASS_LONG_TASKS=1 ENV_FILE=.testing.env jest",
    "test:full": "env ENV_FILE=.testing.env jest",
    "test:watch": "env PASS_LONG_TASKS=1 jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env ENV_FILE=.testing.env TEST_DB=1 jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./npm install -g node_modules/typeorm/cli.js"
  },
  "resolutions": {
    "swagger-ui-dist": "3.30.0"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/typeorm": "^7.1.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "jose": "^1.27.2",
    "jsonwebtoken": "^8.5.1",
    "lodash.omit": "^4.5.0",
    "moment": "^2.27.0",
    "node-rsa": "^1.0.8",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pretty-format": "^26.4.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.4.1",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/lodash.omit": "^4.5.6",
    "@types/node": "^13.9.1",
    "@types/node-rsa": "^1.0.0",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/pretty-format": "^24.3.0",
    "@types/sqlite3": "^3.1.6",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.3.0",
    "jest": "26.0.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^test/(.*)$": "<rootDir>/test/$1"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "node_modules",
      "src/orm/migrations",
      "test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && pretty-quick --staged && env PASS_LONG_TASKS=1 npm run test",
      "pre-push": "npm run test"
    }
  }
}
